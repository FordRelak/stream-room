{
    "name": "stream-room",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "gen": "graphql-codegen --config graphql-codegen.yml",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint --fix",
        "scsslint": "npx stylelint \"**/*.scss\" --fix",
        "prepare": "cd .. && husky install ui/.husky && npx husky add ui/.husky/pre-commit \"cd ui && npx lint-staged\""
    },
    "lint-staged": {
        "*.{ts,html}": "eslint --fix",
        "*.{css,scss}": "stylelint --fix",
        "*.{ts,html,css,scss}": "prettier -w"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.0.1",
        "@angular/cdk": "^15.0.0",
        "@angular/common": "^15.0.1",
        "@angular/compiler": "^15.0.1",
        "@angular/core": "^15.0.1",
        "@angular/forms": "^15.0.1",
        "@angular/material": "^15.0.0",
        "@angular/platform-browser": "^15.0.1",
        "@angular/platform-browser-dynamic": "^15.0.1",
        "@angular/router": "^15.0.1",
        "@apollo/client": "^3.0.0",
        "@ngxs/store": "^3.7.6",
        "apollo-angular": "^4.1.1",
        "graphql": "^16.6.0",
        "graphql-ws": "^5.11.2",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.1",
        "@angular-eslint/builder": "15.1.0",
        "@angular-eslint/eslint-plugin": "15.1.0",
        "@angular-eslint/eslint-plugin-template": "15.1.0",
        "@angular-eslint/schematics": "15.1.0",
        "@angular-eslint/template-parser": "15.1.0",
        "@angular/cli": "~15.0.1",
        "@angular/compiler-cli": "^15.0.1",
        "@graphql-codegen/cli": "2.12.1",
        "@graphql-codegen/introspection": "2.2.1",
        "@graphql-codegen/typed-document-node": "^2.3.3",
        "@graphql-codegen/typescript": "2.7.3",
        "@graphql-codegen/typescript-apollo-angular": "3.5.3",
        "@graphql-codegen/typescript-operations": "2.5.3",
        "@ngxs/devtools-plugin": "^3.7.6",
        "@types/jasmine": "~4.0.0",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.28.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.15.0",
        "eslint-plugin-unicorn": "^43.0.2",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "jasmine-core": "~4.3.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "prettier-eslint": "^15.0.1",
        "stylelint": "^14.11.0",
        "stylelint-config-prettier": "^9.0.3",
